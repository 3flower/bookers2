<% if user_signed_in? %>
<div class="search">
<%= form_tag(search_path, :method => 'get') do %>
    <%= text_field_tag :search %>
    <%= select_tag(:job, options_for_select([["user", "users"],["book","books"]]))%>
    <%= select_tag(:option, options_for_select([
    ["完全一致", "1"], ["前方一致", "2"], ["後方一致", "3"], ["部分一致", "4"]]))%>
    <%= submit_tag 'Search' %>
<% end %>
</div>
<% end %>
